{
  "name": "AdfreeReview",
  "description": "sending rating infos to AdfreeReview Backend Server",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "<all_urls>"
  ],
  "browser_action": {
      "default_title": "AdfreeReview",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
